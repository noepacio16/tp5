---
import LinkBtn from "@/components/atoms/LinkBtn.astro";
import { pages } from "@/content/data/navbar.json";
---

<div
  class="navbar flex justify-center gap-[10px] py-2 z-50 dark:bg-zinc-800 bg-zinc-100 opacity-100"
>
  {pages.map((page) => <LinkBtn href={page.link} text={page.name} size="lg" />)}
</div>
